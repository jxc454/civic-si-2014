{
    "name": "open-data",
    "version": "1.0.0",
    "description": "open data projects",
    "main": "server.js",
    "author": "jxc454",
    "license": "ISC",
    "scripts": {
        "migrate-local": "NODE_ENV=development make migrate-up",
        "migrate-prod": "NODE_ENV=production make migrate-up",
        "dev-db": "make dev-data",
        "clean": "rm -rf dist && npm cache clean -force",
        "build": "tsc",
        "watch:build": "tsc --watch",
        "watch:server": "nodemon -r dotenv/config './dist/server.js' --watch './dist'",
        "start": "npm-run-all migrate-local dev-db --parallel watch:build watch:server --print-label",
        "start-server": "node './dist/server.js'",
        "start-prod": "npm-run-all migrate-prod clean build start-server",
        "build:zip": "webpack && zip -r ./bin/lambda.zip ./bin/lambda.js"
    },
    "dependencies": {
        "apollo-server-express": "^2.14.2",
        "apollo-server-lambda": "^2.9.9",
        "class-validator": "^0.10.1",
        "cors": "^2.8.5",
        "db-migrate": "^0.11.6",
        "db-migrate-pg": "^1.0.0",
        "express": "^4.17.1",
        "graphql": "^14.5.8",
        "lodash": "^4.17.15",
        "pg-native": "^3.0.0",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^0.17.5",
        "typeorm": "^0.2.19",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/preset-env": "^7.6.3",
        "@types/express": "^4.17.1",
        "@types/graphql": "^14.5.0",
        "@types/node": "^12.7.5",
        "aws-xray-sdk": "^2.4.0",
        "babel-loader": "^8.0.6",
        "dotenv": "^8.2.0",
        "nodemon": "^1.19.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.18.2",
        "serverless": "^1.56.1",
        "serverless-offline": "^5.12.0",
        "serverless-webpack": "^5.3.1",
        "ts-node": "^8.3.0",
        "tslint": "^5.20.0",
        "typescript": "^3.6.3",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-filter-warnings-plugin": "^1.2.1"
    }
}
